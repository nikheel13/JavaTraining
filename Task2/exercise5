package Task2;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {


//Exercise 5
        int namba = 0;
        int mod = 0;
        while (namba <= 6){

            mod = namba % 3;

            if(mod != 0){
                System.out.println(namba);
            }
            namba ++;
        }
    }
  }
