package taskSix;

import java.sql.SQLOutput;

class Student{

    String name;


    public Student(String n){

        name = n;
    }

    public Student(){

        name = "Unknown!";
    }
}


public class Main {

    public static void main(String[] args){

        Student n = new Student("King Geofrey!");
        Student f = new Student();

        System.out.println("Name : " + n.name);
        System.out.println("Name : " + f.name);


    }

}
