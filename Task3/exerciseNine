package taskThree;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        {
            //Exercise 9
            //Right angle Triangle
            int i, j, n;
            System.out.print("Enter the number of rows : ");
            Scanner in = new Scanner(System.in);
            n = in.nextInt();

            for (i = 1; i <= n; i++) {
                for (j = 1; j <= i; j++)
                    System.out.print(j);

                System.out.println("");
            }
        }
    }
}
