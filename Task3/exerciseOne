package Task2;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        // Task Three!

        //Exercise 1

        Scanner in = new Scanner(System.in);
        System.out.println("Please enter a number :");
        int number = in.nextInt();

        for(int i =0; i < 12; i++){

            System.out.println(number + "X" + (i+1) + "=" + (number * (i+1)));
        }

    }
}
